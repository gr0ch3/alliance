ENTITY inv_x8 IS
GENERIC (
  CONSTANT area 	 : NATURAL := 175000;
  CONSTANT transistors	 : NATURAL := 8;
  CONSTANT cin_i	 : NATURAL := 54;
  CONSTANT tplh_i_nq	 : NATURAL := 148;
  CONSTANT rup_i_nq	 : NATURAL := 440;
  CONSTANT tphl_i_nq	 : NATURAL := 91;
  CONSTANT rdown_i_nq	 : NATURAL := 400
);
PORT (
  i	 : in  BIT;
  nq	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END inv_x8;

ARCHITECTURE VBE OF inv_x8 IS

BEGIN
  nq <= not (i);
END;
