ENTITY one_x0 IS
GENERIC (
  CONSTANT area 	 : NATURAL := 75000;
  CONSTANT transistors	 : NATURAL := 1
);
PORT (
  q	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END one_x0;

ARCHITECTURE VBE OF one_x0 IS

BEGIN
  ASSERT ((vdd and not (vss)) = '1')
  REPORT "power supply is missing on one_x0"
  SEVERITY WARNING;
  q <= '1';
END;
