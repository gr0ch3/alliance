ENTITY inv_x1 IS
GENERIC (
  CONSTANT area 	 : NATURAL := 75000;
  CONSTANT transistors	 : NATURAL := 2;
  CONSTANT cin_i	 : NATURAL := 8;
  CONSTANT tplh_i_nq	 : NATURAL := 147;
  CONSTANT rup_i_nq	 : NATURAL := 3710;
  CONSTANT tphl_i_nq	 : NATURAL := 109;
  CONSTANT rdown_i_nq	 : NATURAL := 3610
);
PORT (
  i	 : in  BIT;
  nq	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END inv_x1;

ARCHITECTURE VBE OF inv_x1 IS

BEGIN
  nq <= not (i);
END;
