ENTITY inv_x4 IS
GENERIC (
  CONSTANT area 	 : NATURAL := 100000;
  CONSTANT transistors	 : NATURAL := 4;
  CONSTANT cin_i	 : NATURAL := 26;
  CONSTANT tplh_i_nq	 : NATURAL := 161;
  CONSTANT rup_i_nq	 : NATURAL := 1060;
  CONSTANT tphl_i_nq	 : NATURAL := 74;
  CONSTANT rdown_i_nq	 : NATURAL := 800
);
PORT (
  i	 : in  BIT;
  nq	 : out BIT;
  vdd	 : in  BIT;
  vss	 : in  BIT
);
END inv_x4;

ARCHITECTURE VBE OF inv_x4 IS

BEGIN
  nq <= not (i);
END;
